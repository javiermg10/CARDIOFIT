<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.cardiofit">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".mylistcuenta"></activity>
        <activity android:name=".VentanaRegistroEntrenamiento" />
        <activity android:name=".mylist" />
        <activity android:name=".VentanaHistorialActividades" />
        <activity android:name=".VentanaPerfil" />
        <activity android:name=".VentanaAjustes" />
        <activity android:name=".EjemplosDietas" />
        <activity android:name=".EjercicioGluteos" />
        <activity android:name=".EjercicioAbdominales" />
        <activity android:name=".EjercicioResistencia" />
        <activity android:name=".EjercicioTonificacion" />
        <activity android:name=".EjercicioEstiramientos" />
        <activity android:name=".EjercicioCuadriceps" />
        <activity android:name=".EjercicioCrossfit" />
        <activity android:name=".MenuPrincipal" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".VentanaRegistro" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>